{
  "name": "@guren/inertia-client",
  "version": "0.2.0-alpha.6",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gurenjs/guren",
    "directory": "packages/inertia-client"
  },
  "homepage": "https://github.com/gurenjs/guren#readme",
  "bugs": {
    "url": "https://github.com/gurenjs/guren/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./app": {
      "types": "./dist/app.d.ts",
      "import": "./dist/app.js",
      "default": "./dist/app.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "default": "./dist/server.js"
    },
    "./dist/*": {
      "default": "./dist/*"
    },
    "./package.json": {
      "default": "./package.json"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup"
  },
  "dependencies": {
    "@inertiajs/core": "^2.2.15",
    "@inertiajs/react": "^2.2.15",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "typescript": "^5.4.0"
  }
}