{
  "name": "@guren/cli",
  "version": "0.2.0-alpha.6",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gurenjs/guren",
    "directory": "packages/cli"
  },
  "homepage": "https://github.com/gurenjs/guren#readme",
  "bugs": {
    "url": "https://github.com/gurenjs/guren/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./bin": {
      "default": "./dist/bin.js"
    },
    "./vite": {
      "types": "./dist/vite/index.d.ts",
      "default": "./dist/vite/index.js"
    }
  },
  "bin": {
    "guren": "./dist/bin.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "bun --watch src/index.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/parser": "^7.24.7",
    "@guren/orm": "^0.2.0-alpha.6",
    "@guren/server": "^0.2.0-alpha.6",
    "citty": "^0.1.6",
    "consola": "^3.4.2"
  },
  "peerDependencies": {
    "vite": ">=7.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "vite": "^7.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.4.0"
  }
}